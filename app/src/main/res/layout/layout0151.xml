<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:gravity="center_vertical" android:layout_gravity="center" android:orientation="horizontal" android:id="@+id/row_content" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.gbwhatsapp.checkbox.RtlCheckBox android:id="@+id/call_selected_checkbox" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginStart="24dp"/>
        <com.gbwhatsapp.WaImageView android:id="@+id/call_type_icon" android:layout_width="24dp" android:layout_height="24dp" android:layout_margin="24dp"/>
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.gbwhatsapp.WaTextView android:id="@+id/call_type" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" style="?attr/attr0564"/>
                <com.gbwhatsapp.WaTextView android:id="@+id/call_duration" android:paddingTop="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" style="?attr/attr0563"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.gbwhatsapp.WaTextView android:id="@+id/call_date" android:layout_width="wrap_content" android:layout_height="wrap_content" android:drawablePadding="@dimen/dimen0173" style="?attr/attr0563"/>
                <com.gbwhatsapp.WaTextView android:id="@+id/caller_info" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginEnd="@dimen/dimen016e" style="?attr/attr0563"/>
                <com.gbwhatsapp.WaTextView android:id="@+id/call_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen016e" android:layout_marginRight="@dimen/dimen016e" style="?attr/attr0563"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
